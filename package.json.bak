{
  "name": "photography-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "check-package-json": "node scripts/check-package-json.cjs",
    "check:ts": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{js,ts,jsx,tsx}\" --fix",
    "prebuild": "node scripts/check-package-json.cjs && npm run lint",
    "prepare": "husky install",
    "build": "vite build",
    "preview": "vite preview"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@tanstack/react-query": "^5.90.14",
    "@vercel/analytics": "^1.6.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.479.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@types/d3-array": "^3.2.2",
    "@types/d3-color": "^3.1.3",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-timer": "^3.0.2",
    "@types/node": "^22.10.6",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^5.4.19"
  },
  "engines": {
    "node": "18.x"
  }
}
